            <Grid.ColumnDefinitions>
                <ColumnDefinition x:Uid="ColumnDefinition_1" Width="Auto"/>
                <ColumnDefinition x:Uid="ColumnDefinition_2" Width="*"/>
                <ColumnDefinition x:Uid="ColumnDefinition_3" Width="Auto"/>
                <ColumnDefinition x:Uid="ColumnDefinition_4" Width="*"/>
            </Grid.ColumnDefinitions>
            <TextBlock x:Uid="TextBlock_1" Text="간격" Margin="8" Grid.Column="0"/>
            <wtex:DecimalUpDown x:Uid="TileGapDecimalUpDown" x:Name="TileGapDecimalUpDown" Margin="8" Grid.Column="1" />
            <TextBlock x:Uid="TextBlock_2" Text="단위" Margin="8" Grid.Column="2"/>
            <ComboBox x:Uid="TileMeasureTypeComboBox" x:Name="TileMeasureTypeComboBox" Margin="8" Grid.Column="3" SelectionChanged="TileMeasureTypeComboBox_SelectionChanged"></ComboBox>
        </Grid>
    </GroupBox>

